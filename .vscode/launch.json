{
	"version": "0.2.0",
	"compounds": [
	  {
		"name": "all",
		"configurations": ["server", "client"]
	  }
	],
	"configurations": [
	  {
		"name": "client",
		"type": "extensionHost",
		"request": "launch",
		"runtimeExecutable": "${execPath}",
		"args": ["--extensionDevelopmentPath=${workspaceRoot}"],
		"internalConsoleOptions": "neverOpen",
		"sourceMaps": true,
		"outFiles": ["${workspaceRoot}/dist/**/*.js"]
	  },
	  {
		"name": "server",
		"type": "node",
		"request": "attach",
		"port": 6006,
		"sourceMaps": true,
		"outFiles": ["${workspaceRoot}/server/dist/**/*.js"],
		"protocol": "inspector"
	  }
	]
  }